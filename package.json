{
  "name": "tamrenna",
  "version": "1.0.0",
  "description": "Tamrenna Gym Management System",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "build": "npm run build --workspace=client",
    "build:client": "npm run build --workspace=client",
    "build:server": "npm run build --workspace=server",
    "start": "npm run start --workspace=server",
    "test": "npm run test --workspace=client && npm run test --workspace=server",
    "lint": "npm run lint --workspace=client && npm run lint --workspace=server",
    "db:migrate": "npm run db:migrate --workspace=server",
    "db:generate": "npm run db:generate --workspace=server",
    "db:seed": "npm run db:seed --workspace=server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R2Vve/tamrenna.git"
  },
  "keywords": [
    "gym",
    "management",
    "fitness",
    "react",
    "nodejs",
    "prisma"
  ],
  "author": "",
  "license": "MIT"
}